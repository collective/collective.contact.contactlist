<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    xmlns:zcml="http://namespaces.zope.org/zcml"
    xmlns:plone="http://namespaces.plone.org/plone"
    i18n_domain="collective.contact.contactlist">

  <browser:viewlet
      name="collective.contact.facetednav.batchactions.addtolist"
      manager="collective.contact.facetednav.browser.actions.manager.IBatchActions"
      class="collective.contact.contactlist.browser.facetednav.AddToListAction"
      permission="zope2.View"
      />

  <browser:viewlet
      name="collective.contact.facetednav.batchactions.replacelist"
      manager="collective.contact.facetednav.browser.actions.manager.IBatchActions"
      class="collective.contact.contactlist.browser.facetednav.ReplaceListAction"
      permission="zope2.View"
      />

</configure>
